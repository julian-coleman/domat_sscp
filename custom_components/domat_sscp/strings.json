{
  "common": {
    "connection_name": "Connection name",
    "sscp_address": "SSCP address",
    "scan_interval": "Polling interval (seconds)",
    "fast_interval": "Fast polling interval (seconds)",
    "fast_count": "Number of times to run fast polling",
    "write_retries": "Number of retries for entity writes",
    "device": "Device name",
    "name": "Name",
    "uid": "UID",
    "length": "Length",
    "offset": "Offset",
    "minimum": "Minimum",
    "maximum": "Maximum",
    "step": "Step"
  },
  "config": {
    "step": {
      "user": {
        "data": {
          "connection_name": "[%key:component::domat_sscp::common::connection_name%]",
          "ip_address": "[%key:common::config_flow::data::ip%]",
          "port": "[%key:common::config_flow::data::port%]",
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]",
          "sscp_address": "[%key:component::domat_sscp::common::sscp_address%]"
        }
      },
      "reconfigure": {
        "data": {
          "connection_name": "[%key:component::domat_sscp::common::connection_name%]",
          "ip_address": "[%key:common::config_flow::data::ip%]",
          "port": "[%key:common::config_flow::data::port%]",
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]",
          "sscp_address": "[%key:component::domat_sscp::common::sscp_address%]"
        }
      }
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "invalid_auth": "[%key:common::config_flow::error::invalid_auth%]",
      "timeout_connect": "[%key:common::config_flow::error::timeout_connect%]",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    },
    "abort": {
      "already_configured": "[%key:common::config_flow::abort::already_configured_device%]",
      "reconfigure_successful": "[%key:common::config_flow::abort::reconfigure_successful%]",
      "wrong_plc": "The reconfigured device is not the same as the one already configured"
    }
  },
  "options": {
    "step": {
      "init": {
        "title": "Add SSCP devices",
        "menu_options": {
          "temp_hum": "Temperature and humidity",
          "energy": "Energy meters",
          "air": "Air ventilation"
        }
      },
      "temp_hum": {
        "title": "Add a temperature and humidity device",
        "data": {
          "device": "[%key:component::domat_sscp::common::device%]"
        },
        "sections": {
          "temperature_target": {
            "name": "Target temperature:",
            "data": {
              "name": "[%key:component::domat_sscp::common::name%]",
              "uid": "[%key:component::domat_sscp::common::uid%]",
              "minimum": "[%key:component::domat_sscp::common::uid%]",
              "maximum": "[%key:component::domat_sscp::common::uid%]",
              "step": "[%key:component::domat_sscp::common::uid%]"
            }
          },
          "temperature": {
            "name": "Temperature:",
            "data": {
              "name": "[%key:component::domat_sscp::common::name%]",
              "uid": "[%key:component::domat_sscp::common::uid%]"
            }
          },
          "humidity": {
            "name": "Humidity:",
            "data": {
              "name": "[%key:component::domat_sscp::common::name%]",
              "uid": "[%key:component::domat_sscp::common::uid%]"
